{
  "env": {
    "browser": true,
  },
  "extends": ["airbnb", "prettier"],
  "parser": "babel-eslint",
  "plugins": ["prettier"],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "prettier/prettier": ["error"]
  },
  "overrides": [{
    "files": [ "functions/*.js" ],
    "rules": {
      "no-console": "off",
    }
  }]
}
